@model GestionIntervention.Models.Technicien

@{
    ViewData["Title"] = "Supprimer le technicien";
}

<h1>@ViewData["Title"]</h1>

<h3>Êtes-vous sûr de vouloir supprimer ce technicien?</h3>
<div>
    <h4>Technicien</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Code
        </dt>
        <dd class="col-sm-10">
            @Model.CodeTech
        </dd>
        <dt class="col-sm-2">
            Nom
        </dt>
        <dd class="col-sm-10">
            @Model.Nom
        </dd>
        <dt class="col-sm-2">
            Prénom
        </dt>
        <dd class="col-sm-10">
            @Model.Prenom
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="CodeTech" />
        <input type="submit" value="Supprimer" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-secondary">Retour à la liste</a>
    </form>
</div>